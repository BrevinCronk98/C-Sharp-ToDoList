@using ToDoList.Models;

<h3> Here are all the items in this category</h3>

<ol>
@foreach (Item item in @Model["items"])
{
    <li><a href='/categories/@Model["category"].Id/items/@item.Id'>@item.Description</a></li>
}
</ol>

<h3><a href='/categories/@Model["category"].Id/items/new'> Add a new Item</a></h3>
<h3><a href='/categories'>Return to Category List</a></h3>
<h3><a href='/'> Return Home</a></h3>